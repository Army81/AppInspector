{
    "manifest_version": 2,
    "minimum_chrome_version": "31.0",
    "name": "__MSG_appName__",
    "version": "2.0.0",
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "AppInspector/resources/images/icon-16x16.png",
        "48": "AppInspector/resources/images/icon-48x48.png",
        "128": "AppInspector/resources/images/icon-128x128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": "background.js"
    },
    "devtools_page": "devtools-page.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
        "tabs",
        "<all_urls>",
        "experimental"
    ]
}
